<launch>
<!-- Launch file for ouster OS2-64 LiDAR -->

    <arg name="rviz" default="false" />
    <arg name="sim" default="false" />
    <arg name="robot_name" default="" />
    <arg name="frame_prefix" default="$(arg robot_name)/" />

    <rosparam command="load" file="$(find fast_lio)/config/ouster64.yaml" />
    
    <param name="feature_extract_enable" type="bool" value="false"/>
    <param name="point_filter_num" type="int" value="4"/>
    <param name="max_iteration" type="int" value="3" />
    <param name="filter_size_surf" type="double" value="0.5" />
    <param name="filter_size_map" type="double" value="0.5" />
    <param name="cube_side_length" type="double" value="1000" />
    <param name="runtime_pos_log_enable" type="bool" value="false" />
    <param name="use_sim" type="bool" value="$(arg sim)" />
    <param name="pcd_save/pcd_save_en" type="bool" value="false" /> <!-- save frame: spot/map -->

    <arg name="launch_prefix" default="bash -c 'python3 $(find spot_navigation)/src/wait_spot_start.py {};$0 $@' "/>
    <node pkg="fast_lio" type="fastlio_mapping" name="laserMapping" output="screen" 
        launch-prefix="$(arg launch_prefix)"/>   

    <!-- if using 2d_map + amcl, comment below code -->
    <!--
    <arg name="pcd_map" default="$(find fast_lio_localization)/PCD/scans.pcd" />
    <node pkg="pcl_ros" type="pcd_to_pointcloud" name="map_publishe" output="screen"
          args="$(arg pcd_map) 5 _frame_id:=$(arg frame_prefix)map cloud_pcd:=pcd_map" />
    -->
    
    <!-- 
    <group if="$(arg rviz)">
        <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find fast_lio)/rviz_cfg/loam_livox.rviz" />
    </group> 
    -->

    <!-- <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="cloud_body_to_laser" output="screen">
        <param name="target_frame" value="$(arg frame_prefix)base_link" />
        <remap from="cloud_in" to="cloud_registered_body3" />
    </node> -->

</launch>
